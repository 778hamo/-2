    {
      "cell_type": "code",
      "source": [
        "lin_reg = LinearRegression()\n",
        "lin_reg.fit(X_train_scaled, y_train)\n",
        "mse_train_lin = mean_squared_error(y_train, lin_reg.predict(X_train_scaled))\n",
        "mse_test_lin = mean_squared_error(y_test, lin_reg.predict(X_test_scaled))\n",
        "\n",
        "alphas = [0.01, 0.1, 1, 10]\n",
        "ridge_results = []\n",
        "for a in alphas:\n",
        "    ridge = Ridge(alpha=a)\n",
        "    ridge.fit(X_train_scaled, y_train)\n",
        "    ridge_results.append({\n",
        "        'Model': f'Ridge (Œ±={a})',\n",
        "        'Train MSE': mean_squared_error(y_train, ridge.predict(X_train_scaled)),\n",
        "        'Test MSE': mean_squared_error(y_test, ridge.predict(X_test_scaled))\n",
        "    })\n",
        "\n",
        "lasso_results = []\n",
        "for a in alphas:\n",
        "    lasso = Lasso(alpha=a)\n",
        "    lasso.fit(X_train_scaled, y_train)\n",
        "    lasso_results.append({\n",
        "        'Model': f'Lasso (Œ±={a})',\n",
        "        'Train MSE': mean_squared_error(y_train, lasso.predict(X_train_scaled)),\n",
        "        'Test MSE': mean_squared_error(y_test, lasso.predict(X_test_scaled))\n",
        "    })\n",
        "\n",
        "results = [\n",
        "    {'Model': 'Linear Regression', 'Train MSE': mse_train_lin, 'Test MSE': mse_test_lin}\n",
        "] + ridge_results + lasso_results\n",
        "\n",
        "results_df = pd.DataFrame(results)\n",
        "results_df"
